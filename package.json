{
  "name": "website-etchebades",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.5"
  },
  "scripts": {
    "dev": "next dev",
    "bootstrap": "./scripts/clean.sh && npm install",
    "build": "next build",
    "serve": "next start",
    "compile": "npx tsc",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:js-fix && npm run lint:style-fix",
    "lint:js": "next lint",
    "lint:js-fix": "next lint --fix",
    "lint:style": "stylelint \"**/*.{scss,css}\" --ignore-path .gitignore",
    "lint:style-fix": "npm run lint:style -- --fix",
    "start": "next start",
    "validate": "npm run lint && npm run compile"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "sass": "^1.69.6",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "typescript": "^5"
  }
}
