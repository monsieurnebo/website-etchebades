{
  "name": "website-etchebades",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.13.1",
    "npm": ">=8.1.2"
  },
  "scripts": {
    "start": "next dev -p 8080",
    "bootstrap": "./scripts/clean.sh && npm install",
    "build": "next build",
    "serve": "next start",
    "compile": "npx tsc",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:js-fix && npm run lint:style-fix",
    "lint:js": "next lint",
    "lint:js-fix": "next lint --fix",
    "lint:style": "stylelint \"**/*.{js,scss,css}\" --ignore-path .gitignore",
    "lint:style-fix": "npm run lint:style -- --fix",
    "validate": "npm run lint && npm run compile"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "next": "^11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.38.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.27",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
    "typescript": "^4.4.3"
  }
}
